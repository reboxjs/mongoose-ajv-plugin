{
  "name": "@cdmbase/mongoose-ajv-plugin",
  "version": "2.0.0",
  "description": "AJV plugin for Mongoose",
  "keywords": [
    "mongoose",
    "ajv",
    "json-schema",
    "json",
    "schema",
    "JSONschema"
  ],
  "homepage": "https://github.com/jdthorpe/mongoose-ajv-plugin#readme",
  "bugs": {
    "url": "https://github.com/jdthorpe/mongoose-ajv-plugin/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jdthorpe/mongoose-ajv-plugin.git"
  },
  "license": "ISC",
  "author": "Jason Thorpe",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "watch": "tsc -w"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "roots": [
      "src"
    ],
    "testEnvironment": "node",
    "testRegex": "/__tests__/.*test*\\.(ts|tsx|js)$",
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    }
  },
  "dependencies": {
    "ajv": "^8.0.0",
    "ajv-formats": "^2.1.1"
  },
  "devDependencies": {
    "@types/jest": "^21.1.2",
    "@types/node": "16.6.1",
    "jest": "^26.6.3",
    "jest-cli": "^26.0.0",
    "mongoose": "^6.1.7",
    "ts-jest": "^26.0.0",
    "ts-node": "^10.2.0",
    "typescript": "4.3.5"
  },
  "peerDependencies": {
    "mongoose": ">=4.0.0",
    "semver": ">=5.3.0"
  }
}
